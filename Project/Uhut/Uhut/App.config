<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
	</configSections>

	<log4net>
		<!--定义输出到文件中-->
		<appender name="RegLog" type="log4net.Appender.RollingFileAppender">
			<!--定义文件存放位置-->
			<file value="log\\RegLog.log"/>
			<param name="AppendToFile" value="true"/>
			<param name="RollingStyle" value="Date"/>
			<param name="DatePattern" value=".yyyy-MM-dd"/>
			<param name="StaticLogFileName" value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%d - %m%n"/>
			</layout>
		</appender>
		<root>
			<level value="DEBUG"/>
			<!--文件形式记录日志-->
			<appender-ref ref="RegLog"/>
		</root>
	</log4net>

	<appSettings>
		<add key="IntervalSecond" value="120"/><!--间隔秒-->
		<add key="RegPwd" value="qwert12345"/><!--密码-->
		<add key="ChannelID" value="tiyu-e5b0-4fb9-b21c-0a6933bcd423-ty-34-16"/><!--ChannelID-->
		<add key="Biztp" value="999"/><!--Biztp-->
		<add key="PHPSESSID" value="ivvmebbuju81m7n7bols0jupj5"/>
	</appSettings>
</configuration>